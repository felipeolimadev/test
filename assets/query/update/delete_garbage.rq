DELETE { ?s ?p ?o }
WHERE {
        VALUES (?targetClass) {<TargetClass>}
        ?s rdf:type|(rdf:type/rdfs:subClassOf) ?targetClass .
        ?s ?p ?o .
        FILTER NOT EXISTS {
             VALUES (?class) {<ParentClass>}
             ?something rdf:type/rdfs:subClassOf ?class .
             ?something ?predicate ?s .
        }
}