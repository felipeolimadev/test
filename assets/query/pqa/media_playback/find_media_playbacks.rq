SELECT DISTINCT ?s ?p ?o {
    {
        ?activity rdf:type ?targetActivityType .
<TARGET_TYPES_FILTER>
        ?activity time:hasBeginning/time:inXSDDateTimeStamp ?start_time .
        ?activity time:hasEnd/time:inXSDDateTimeStamp ?end_time .
        ?activity user:objectContent ?s .
<PERSON_FILTER>
<LOCATION_FILTER>
<TIME_FILTER>
<EXERCISING_FILTER>
<EATING_FILTER>
    }
    ?s ?p ?o .
} <OUTPUT_SORT>