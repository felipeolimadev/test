    "isPrefix" : true,
    "isOnlyPrefix" : true
  },
  {
    "id" : 5,
    "type" : "RELATIVE_DAY_OF_WEEK",
    "patternString" : "(<after_range>|<before_range>|<current_reference>|<last_reference>)\\s*(<day_of_week>)",
    "comment" : "last sunday, next sunday",
    "isRelative": true
  }
