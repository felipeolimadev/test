[
  {
    "id": 1,
    "type": "NUM_TIME_AFTER_REFERENCE",
    "comment": "2 days from now, a week after, after a week, past 2 days, before 3 days, etc.(सोमवार के 10 सप्ताह और 2 दिन बाद)",
    "patternString": "(?:(की|के)\\s*)?((<num>)?\\s*(<time_period>))\\s*(?:और\\s*)?((<num>)?\\s*(<time_period>))?\\s*(<after_reference>|<before_reference>)",
    "isRelative": true,
    "isPrefix": true,
    "isOnlyDelta" : true
  },
  {
    "id": 2,
    "type": "AFTER_REFERENCE_NUM_TIME",
    "comment": "after a week, before 2 months",
    "patternString": "((<num>)?\\s*(<time_period>))\\s*(?:और\\s*)?(?:a|an)?\\s*((<num>)?\\s*(<time_period>))?\\s*(?:और\\s*)?((<num>)?\\s*(<time_period>))?\\s*(?:(की|के)\\s*)?(<after_reference>|<before_reference>)",
    "isRelative": true,
    "isPrefix" : true,
    "isOnlyDelta" : true
  },
  {
    "id" : 3,
    "type" : "AFTER_REFERENCE_NUM_TIME_ONLY_FOR_PREFIX",
    "comment" : "next month, next year, this will only serve as a prefix | suffix for cases like 25th jan next year, only next year will be handled by range resolver.",
    "patternString" : "(?:(के|की)\\s*)?(<after_range>|<before_range>|<last_reference>|<current_reference>)\\s*(<time_period>)(?:\\s*(के|की))?",
